{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "description": "<%= projectDesc %>",
  "main": "",
  "scripts": {
    "clean": "rm -rf dist/*",
    "check": "eslint src test && stylelint 'src/**/*.css'",
    "test": "NODE_PATH=$NODE_PATH:$PWD/src mocha --compilers js:babel-core/register,js:babel-polyfill,css:test/css-null-compiler.js --recursive",
    "webpack:client-prod": "NODE_ENV=production webpack --config webpack/client.js --progress ",
<% if (serverSide) { -%>
    "webpack:server-prod": "NODE_ENV=production webpack --config webpack/server.js --progress && cp -r node_modules dist/",
    "webpack:server-dev": "NODE_ENV=development webpack --config webpack/server.js --progress",
    "dist": "npm run clean && npm run webpack:client-prod && npm run webpack:server-prod",
    "start": "NODE_ENV=development webpack-dashboard -- nodemon --watch src/server --exec 'npm run clean && npm run webpack:server-dev && node devServer.js'",
    "start:dist": "npm run dist && cd dist && node server.js",
<% } else { -%>
    "dist": "npm run clean && npm run webpack:client-prod",
    "start": "CLIENT_PORT=8000 NODE_ENV=development webpack-dashboard -- webpack-dev-server --inline --config webpack/client.js",
    "start:dist": "npm run dist && cp index.html dist/ && cd dist && open index.html",
<% } -%>
    "precommit": "npm run check && npm test",
    "commitmsg": "node validate-commit-msg.js"
  },
  "dependencies": {
    "antd": "2.0.1",
    "bluebird": "3.4.6",
    "classnames": "2.2.5",
<% if (serverSide) { -%>
    "h2o2": "5.4.0",
    "hapi": "15.1.1",
    "inert": "4.0.2",
<% } -%>
    "immutable": "3.8.1",
    "isomorphic-style-loader": "1.0.0",
    "normalize.css": "5.0.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-import": "1.0.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-runtime": "6.11.6",
    "chai": "3.5.0",
    "css-loader": "0.25.0",
    "enzyme": "2.4.1",
    "eslint": "3.8.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-import-resolver-webpack": "0.6.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "findup": "0.1.5",
    "husky": "0.11.9",
    "jsdom": "9.7.1",
    "mocha": "2.5.3",
    "nodemon": "1.11.0",
    "postcss": "5.2.4",
    "postcss-loader": "0.13.0",
    "react-addons-test-utils": "15.3.2",
    "react-hot-loader": "1.3.0",
    "semver-regex": "1.0.0",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "stylelint": "7.4.2",
    "stylelint-webpack-plugin": "0.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dashboard": "0.2.0",
    "webpack-dev-server": "1.16.2",
    "whatwg-fetch": "1.0.0"
  },
  "engines": {
    "node": "6.6.0",
    "npm": "3.10.3"
  }
}
